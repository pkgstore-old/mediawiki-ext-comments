  },
  "require": {
    "php": ">=7.2",
    "composer/installers": "^1.0.1",
    "pkgstore/mediawiki-ext-kernel": "*"
  },
  "autoload": {
    "classmap": [
      "MW_EXT_Comments.class.php"
    ]
  },
  "config": {
    "optimize-autoloader": true,
