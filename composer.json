{
  "name": "pkgstore/mediawiki-ext-comments",
  "type": "mediawiki-extension",
  "description": "MediaWiki comments extension.",
  "license": "MIT",
  "homepage": "https://pkgstore.github.io/",
  "keywords": [
    "mediawiki",
    "comments"
  ],
  "authors": [
    {
      "name": "Kitsune Solar",
      "email": "kitsune.solar@gmail.com",
      "homepage": "https://kitsune.solar/",
      "role": "Developer"
    }
  ],
  "support": {
